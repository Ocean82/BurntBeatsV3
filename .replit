args = "node quick-production-fix.cjs"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd dist"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install --production"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node index.js"

